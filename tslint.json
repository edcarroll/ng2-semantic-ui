{
  "rulesDirectory": [
    "node_modules/codelyzer",
    "node_modules/vrsource-tslint-rules/rules"
  ],
  "rules": {
    "align": [
      true,
      "arguments",
      "elements",
      "members",
      "parameters",
      "statements"
    ],
    "array-type": [
      true,
      "array"
    ],
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "arrow-return-shorthand": true,
    "await-promise": true,
    "binary-expression-operand-order": true,
    "callable-types": true,
    "comment-format": [
      true,
      "check-space"
    ],
    "conditional-expression-parens": false,
    "curly": true,
    "encoding": true,
    "eofline": true,
    "ext-variable-name": [
      true,
      ["class", "pascal"],
      ["interface", "pascal", { "regex": "^I.*$" }],
      ["parameter", "camel"],
      ["property", "static", "camel"],
      ["property", "private", "camel", "require-leading-underscore"],
      ["property", "protected", "camel", "require-leading-underscore"],
      ["variable", "camel"],
      ["method", "camel"],
      ["function", "camel"],
      ["default", "camel"]
    ],
    "forin": true,
    "import-blacklist": [true, "rxjs"],
    "import-spacing": true,
    "indent": [
      true,
      "spaces",
      4
    ],
    "interface-over-type-literal": true,
    "jsdoc-format": true,
    "label-position": true,
    "linebreak-style": [
      true,
      "CRLF"
    ],
    "literal-spacing": [
      true,
      {
        "array": ["never"],
        "object": ["always"],
        "import": ["always"]
      }
    ],
    "max-line-length": [
      true,
      140
    ],
    "member-access": true,
    "member-ordering": [
      true,
      {
        "order": [
          "static-field",
          "instance-field",
          "constructor",
          "static-method",
          "instance-method"
        ]
      }
    ],
    "new-parens": true,
    "no-angle-bracket-type-assertion": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-boolean-literal-compare": true,
    "no-console": [
      true,
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "no-conditional-assignment": true,
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-imports": true,
    "no-duplicate-super": true,
    "no-duplicate-variable": true,
    "no-empty": false,
    "no-empty-interface": true,
    "no-eval": true,
    "no-floating-promises": true,
    "no-increment-decrement": true,
    "no-inferrable-types": [
      true,
      "ignore-params",
      "ignore-properties"
    ],
    "no-inferred-empty-object-type": true,
    "no-invalid-template-strings": true,
    "no-invalid-this": true,
    "no-non-null-assertion": true,
    "no-object-literal-type-assertion": true,
    "no-param-reassign": true,
    "no-reference": true,
    "no-shadowed-variable": true,
    "no-sparse-arrays": true,
    "no-string-literal": false,
    "no-string-throw": true,
    "no-switch-case-fall-through": true,
    "no-trailing-whitespace": [
      true,
      "ignore-template-strings"
    ],
    "no-unused-expression": true,
    "no-use-before-declare": true,
    "no-var-keyword": true,
    "no-var-requires": true,
    "number-literal-format": true,
    "object-literal-sort-keys": false,
    "one-line": [
      true,
      "check-catch",
      "check-finally",
      "check-else",
      "check-open-brace",
      "check-whitespace"
    ],
    "one-variable-per-declaration": [
      true,
      "ignore-for-loop"
    ],
    "only-arrow-functions": [
      true,
      "allow-declarations"
    ],
    "prefer-conditional-expression": true,
    "prefer-const": true,
    "prefer-literal": [
      true,
      "object",
      "function",
      "array"
    ],
    "prefer-object-spread": true,
    "prefer-template": true,
    "promise-function-async": true,
    "quotemark": [
      true,
      "double",
      "avoid-escape"
    ],
    "radix": true,
    "restrict-plus-operands": true,
    "semicolon": [
      "always"
    ],
    "space-before-function-paren": [
      true,
      "never"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "type-literal-delimiter": true,
    "typedef": [
      true,
      "call-signature",
      "parameter",
      "member-variable-declaration"
    ],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      },
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "typeof-compare": true,
    "unified-signatures": true,
    "use-default-type-parameter": true,
    "variable-name": [
      true,
      "ban-keywords"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator"
    ],

    "directive-selector": false,
    "component-selector": [
      true,
      "element",
      ["sui", "demo"],
      "kebab-case"
    ],
    "use-input-property-decorator": true,
    "use-output-property-decorator": true,
    "use-host-property-decorator": false,
    "no-input-rename": true,
    "no-output-rename": true,
    "use-life-cycle-interface": true,
    "use-pipe-transform-interface": true,
    "component-class-suffix": false,
    "directive-class-suffix": false,
    "no-access-missing-member": true,
    "templates-use-public": true,
    "invoke-injectable": true
  }
}
